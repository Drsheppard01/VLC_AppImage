#!/bin/bash
if
HERE=$ORIGIN
then
HERE="$(dirname $(readlink -f "${0}"))"
fi
export UNION_PRELOAD="${HERE}"
export LD_PRELOAD="${HERE}/libunionpreload.so"
export FONTCONFIG_PATH="${HERE}"/etc/fonts:"${HERE}"/etc/fonts/conf.d:"${HERE}"/etc/fonts/conf.avail:"${FONTCONFIG_PATH}"
