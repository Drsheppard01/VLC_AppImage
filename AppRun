VENDOR=$(glxinfo | grep "OpenGL vendor")
if [[ $VENDOR == *"Intel"* ]]; then
  export VDPAU_DRIVER_PATH="${HERE}/usr/lib/x86_64-linux-gnu/dri/"
  export LIBVA_DRIVERS_PATH="${HERE}/usr/lib/x86_64-linux-gnu/dri/"
fi

if [[ $VENDOR == *"NVIDIA"* ]]; then
  export VDPAU_DRIVER_PATH="${HERE}/usr/lib/x86_64-linux-gnu/vdpau/"
elif [[ $VENDOR == *"X.Org"* ]]; then
  export VDPAU_DRIVER_PATH="${HERE}/usr/lib/x86_64-linux-gnu/vdpau/"
fi
export PATH="${HERE}"/usr:"${HERE}"/usr/share:"${HERE}"/usr/share/vlc:"${HERE}"/usr/share/vlc/lua:"${HERE}"/usr/share/vlc/lua/http:"${HERE}"/usr/share/vlc/lua/http/dialogs:"${HERE}"/usr/share/vlc/lua/http/js:"${HERE}"/usr/share/vlc/lua/http/css:"${HERE}"/usr/share/vlc/lua/http/css/ui-lightness:"${HERE}"/usr/share/vlc/lua/http/css/ui-lightness/images:"${HERE}"/usr/share/vlc/lua/http/images:"${HERE}"/usr/share/vlc/lua/http/requests:"${HERE}"/usr/share/vlc/skins2:"${HERE}"/usr/share/vlc/skins2/fonts:"${HERE}"/usr/share/vlc/utils:"${HERE}"/usr/share/bash-completion:"${HERE}"/usr/share/bash-completion/completions:"${HERE}"/usr/share/qlogging-categories5:"${HERE}"/usr/share/alsa:"${HERE}"/usr/share/alsa/topology:"${HERE}"/usr/share/alsa/topology/bxtrt298:"${HERE}"/usr/share/alsa/topology/sklrt286:"${HERE}"/usr/share/alsa/topology/broadwell:"${HERE}"/usr/share/alsa/alsa.conf.d:"${HERE}"/usr/share/alsa/cards:"${HERE}"/usr/share/alsa/cards/SI7018:"${HERE}"/usr/share/alsa/ucm:"${HERE}"/usr/share/alsa/ucm/PandaBoard:"${HERE}"/usr/share/alsa/ucm/tegraalc5632:"${HERE}"/usr/share/alsa/ucm/VEYRON-I2S:"${HERE}"/usr/share/alsa/ucm/tegra-rt5640:"${HERE}"/usr/share/alsa/ucm/PAZ00:"${HERE}"/usr/share/alsa/ucm/LENOVO-MFG_IN_GO-ThinkPad-MFG_IN_GO:"${HERE}"/usr/share/alsa/ucm/GoogleNyan:"${HERE}"/usr/share/alsa/ucm/LENOVO-20QD00LKIX-ThinkPadX1Carbon7th-20QD00LKIX:"${HERE}"/usr/share/alsa/ucm/SDP4430:"${HERE}"/usr/share/alsa/ucm/LENOVO-BIOSID-N2H:"${HERE}"/usr/share/alsa/ucm/chtrt5645:"${HERE}"/usr/share/alsa/ucm/LENOVO-BIOSID-N2Y:"${HERE}"/usr/share/alsa/ucm/LENOVO-BIOSID-N2W:"${HERE}"/usr/share/alsa/ucm/Manta-SPDIF:"${HERE}"/usr/share/alsa/ucm/PandaBoardES:"${HERE}"/usr/share/alsa/ucm/DAISY-I2S:"${HERE}"/usr/share/alsa/ucm/DB410c:"${HERE}"/usr/share/alsa/ucm/broadwell-rt286:"${HERE}"/usr/share/alsa/ucm/LENOVO-20QE000VMC-ThinkPadX1Carbon7th-20QE000VMC:"${HERE}"/usr/share/alsa/ucm/Tuna:"${HERE}"/usr/share/alsa/ucm/skylake-rt286:"${HERE}"/usr/share/alsa/ucm/msm8974-taiko-mtp-snd-card:"${HER